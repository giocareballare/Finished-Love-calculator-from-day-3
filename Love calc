# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†
combinedname= name1 + name2
lowercase=combinedname.lower()
true_score= lowercase.count("t") + lowercase.count("r") + lowercase.count("u") + lowercase.count("e")
love_score= lowercase.count("l") + lowercase.count("o") + lowercase.count("v") + lowercase.count("e")
reallove= int(f"{true_score}{love_score}")
if reallove < 10 or reallove > 90:
  print(f"Your score is {reallove}, you go together like coke and mentos.")
  if reallove > 90:
    print("You suck.")
elif 40 <reallove<50:
  print(f"Your score is {reallove}, you are alright together.")
else:
  print(f"Your score is {reallove}.")


